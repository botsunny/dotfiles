{
    "workbench.colorTheme": "Nord",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "terminal.integrated.inheritEnv": false,
    "editor.parameterHints.enabled": false,
    "editor.fontFamily": "'FiraCode Nerd Font Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "latex-workshop.latex.autoBuild.run": "never",
    "jupyter.allowUnauthorizedRemoteConnection": true,
    "workbench.editor.untitled.hint": "hidden",
    "java.home": "/usr/lib/jvm/java-17-openjdk-amd64",
    "python.venvFolders": [
        "miniconda3/envs"
    ],
    "python.venvPath": "~/miniconda3/envs",
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": true,
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "editor.inlineSuggest.enabled": true,
    "files.associations": {
        "*.rmd": "markdown"
    },
    "r.rpath.linux": "/home/sunny/miniconda3/envs/r-learning/bin/R",
    "r.plot.useHttpgd": true,
    "markdown.preview.fontFamily": "'Noto Sans', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
    "files.hotExit": "off",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "right"
    },
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": false,
        "markdown": false
    },
    "r.rterm.linux": "/home/sunny/miniconda3/envs/r-learning/bin/radian",
    "notebook.outputFontFamily": "'Source Code Pro'",
    
}